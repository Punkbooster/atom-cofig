        {
          name: "app & spec"
          active: true
          uris: [
            "/home/punkbooster/Spark/greetabl/app"
            "/home/punkbooster/Spark/greetabl/spec"
          ]
        }
        {
          name: "db"
          active: true
          uris: [
            "/home/punkbooster/Spark/greetabl/db"
          ]
        }
        {
          name: "lib"
          active: true
          uris: [
            "/home/punkbooster/Spark/greetabl/lib"
          ]
        }
      ]
  welcome:
    showOnStartup: false
