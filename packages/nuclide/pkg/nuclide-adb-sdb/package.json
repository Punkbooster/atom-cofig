{
  "name": "nuclide-adb-sdb",
  "main": "./lib/main.js",
  "version": "0.0.0",
  "description": "Utilities for adb and sdb",
  "author": "NEEDS OWNER",
  "atomTestRunner": "../../lib/test-runner-entry.js",
  "nuclide": {
    "packageType": "Atom",
    "testRunner": "apm",
    "configMetadata": {
      "pathComponents": [
        "IDE",
        "Tizen"
      ]
    },
    "config": {
      "tizen": {
        "title": "Show Tizen devices",
        "type": "boolean",
        "default": "true",
        "description": "Whether to show a Tizen tab in the Devices panel. Restart required."
      }
    }
  },
  "consumedServices": {
    "nuclide.devices": {
      "versions": {
        "0.0.0": "consumeDevicePanelServiceApi"
      }
    }
  }
}
