{
  "name": "nuclide-objc",
  "repository": "https://github.com/facebook/nuclide",
  "main": "./lib/main.js",
  "version": "0.0.0",
  "description": "Provides support for Objective-C in Nuclide",
  "atomTestRunner": "../../lib/test-runner.js",
  "nuclide": {
    "configMetadata": {
      "pathComponents": [
        "Language",
        "Obj-C"
      ]
    },
    "config": {
      "enableAutomaticSquareBracketInsertion": {
        "title": "Enable Automatic Square Bracket Insertion",
        "type": "boolean",
        "default": false
      }
    },
    "packageType": "Atom",
    "testRunner": "apm"
  },
  "activationHooks": [
    "language-objective-c:grammar-used"
  ]
}
