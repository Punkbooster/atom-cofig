{
  "name": "nuclide-refactorizer",
  "repository": "https://github.com/facebook/nuclide",
  "main": "./lib/main.js",
  "version": "0.0.0",
  "description": "Refactorize your code",
  "atomTestRunner": "../../lib/test-runner.js",
  "nuclide": {
    "packageType": "Atom",
    "testRunner": "apm"
  },
  "consumedServices": {
    "nuclide-refactor": {
      "versions": {
        "0.0.0": "consumeRefactorProvider"
      }
    }
  },
  "activationHooks": [
    "nuclide_refactoring:gk:nuclide"
  ]
}
