{
  "name": "nuclide-source-control-side-bar",
  "repository": "https://github.com/facebook/nuclide",
  "main": "./lib/main.js",
  "version": "0.0.0",
  "description": "Source control management in your nuclide-side-bar",
  "atomTestRunner": "../../lib/test-runner.js",
  "nuclide": {
    "configMetadata": {
      "pathComponents": [
        "Source Control",
        "Project Explorer"
      ]
    },
    "config": {
      "createStacked": {
        "title": "Stack features",
        "type": "boolean",
        "default": false,
        "description": "Create bookmarks on the currently active bookmark"
      }
    },
    "packageType": "Atom",
    "testRunner": "apm"
  },
  "consumedServices": {
    "nuclide-side-bar": {
      "versions": {
        "1.0.0": "consumeNuclideSideBar"
      }
    }
  }
}
