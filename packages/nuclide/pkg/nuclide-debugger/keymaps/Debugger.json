{
  ".platform-darwin atom-workspace": {
    "cmd-shift-a": "nuclide-debugger:show-attach-dialog",
    "cmd-f8": "nuclide-debugger:show-launch-dialog",
    "cmd-shift-f8": "nuclide-debugger:restart-debugging"
  },
  ".platform-win32 atom-workspace, .platform-linux atom-workspace": {
    "ctrl-shift-a": "nuclide-debugger:show-attach-dialog",
    "ctrl-f8": "nuclide-debugger:show-launch-dialog",
    "ctrl-shift-f8": "nuclide-debugger:restart-debugging"
  },
  "atom-workspace": {
    "f8": "nuclide-debugger:continue-debugging",
    "shift-f8": "nuclide-debugger:run-to-location",
    "shift-f5": "nuclide-debugger:stop-debugging",
    "f10": "nuclide-debugger:step-over",
    "f11": "nuclide-debugger:step-into",
    "shift-f11": "nuclide-debugger:step-out",
    "f9": "nuclide-debugger:toggle-breakpoint"
  }
}
