{
  "name": "nuclide-patch-editor",
  "repository": "https://github.com/facebook/nuclide",
  "main": "./lib/main.js",
  "version": "0.0.0",
  "description": "Provides a user interface for editing source control patches",
  "atomTestRunner": "../../lib/test-runner-entry.js",
  "nuclide": {
    "packageType": "Atom",
    "testRunner": "apm"
  },
  "consumedServices": {
    "nuclide-current-working-directory": {
      "versions": {
        "0.0.0": "consumeCwdApi"
      }
    }
  },
  "activationHooks": [
    "nuclide_patch_editor:gk:nuclide"
  ]
}
