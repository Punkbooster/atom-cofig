{
  "name": "nuclide-ctags",
  "repository": "https://github.com/facebook/nuclide",
  "main": "./lib/main.js",
  "version": "0.0.0",
  "description": "Exposes nuclide-ctags-rpc functionality to Atom.",
  "atomTestRunner": "../../lib/test-runner.js",
  "nuclide": {
    "configMetadata": {
      "pathComponents": [
        "Language",
        "Remote ctags"
      ]
    },
    "config": {
      "disableWithHack": {
        "title": "Filter out PHP symbols when Hack is available",
        "type": "boolean",
        "default": "true",
        "description": "Avoid overlap with symbol results from Hack."
      }
    },
    "packageType": "Atom",
    "testRunner": "apm"
  },
  "providedServices": {
    "hyperclick.provider": {
      "versions": {
        "0.0.0": "getHyperclickProvider"
      }
    },
    "nuclide-quick-open-register-provider": {
      "versions": {
        "0.0.0": "getQuickOpenProvider"
      }
    }
  }
}
