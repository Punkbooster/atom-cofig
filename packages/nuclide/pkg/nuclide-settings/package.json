{
  "name": "nuclide-settings",
  "repository": "https://github.com/facebook/nuclide",
  "main": "./lib/main.js",
  "version": "0.0.0",
  "description": "A view of Nuclide Settings organized by features instead of by package.",
  "atomTestRunner": "../../lib/test-runner.js",
  "nuclide": {
    "packageType": "Atom",
    "testRunner": "apm"
  },
  "consumedServices": {
    "nuclide.workspace-views": {
      "versions": {
        "0.0.0": "consumeWorkspaceViewsService"
      }
    },
    "tool-bar": {
      "versions": {
        "^1.0.0": "consumeToolBar"
      }
    }
  }
}
